  <link rel="stylesheet" href="{{ app.request.baseUrl ~ '/assets/css/bootstrap.min.css' }}"/>
  <link rel="stylesheet" href="{{ app.request.baseUrl ~ '/assets/css/style.css' }}"/>
  <title>Pages Admin.</title>
</head>
<body>
{{ include('shared/header.twig') }}
<div class="container">
  <div class="row">
